{
  "name": "be-university-tests",
  "version": "1.0.0",
  "description": "Test suite for University Backend API",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:image-deletion": "node --experimental-vm-modules node_modules/jest/bin/jest.js image-deletion.test.js",
    "test:simple": "node --experimental-vm-modules node_modules/jest/bin/jest.js simple-image-deletion.test.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "form-data": "^4.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 30000,
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "transform": {}
  }
}
